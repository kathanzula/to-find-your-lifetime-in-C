// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    printf("ENTER THE NUMBER OF TABLES = ");
    int a;
    scanf("%d",&a);
    for(int i=1;i<=a;i++){
        for(int j=1;j<=10;j++){
            int k = i*j;
            printf("%d x %d = %d",i,j,k);
            printf("\n");
        }
        printf("\n\n\n");
    }

    return 0;
}
